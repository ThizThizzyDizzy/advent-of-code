$env include
$fs include
/unpack {
    { } foreach
} macro
/readDay {
    "../input/day" exch ~ ".txt" ~ fromRoot readString .trim
} macro
/readString {
    "" exch readLines { '\n' ~ ~ } foreach
} macro
/startsWithOffset {
    /offset exch def
    /start exch def
    /str exch def
    
    /str str offset substring =
    start str .startsWith
} macro
/substring {
    /start exch def
    /str exch def
    
    "" str { start 0 > { pop /start 1 -= } { ~ } ifelse } foreach
} macro